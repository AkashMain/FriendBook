
<%= "Logged in as #{current_user.fname}"%><br><br>
<h1>All Users</h1>

<ul>
  <% @users.each do |user| %>
    <li>
      <%if user.profile_picture.attached? %>
        <%= image_tag user.profile_picture, class: "avatar" %>
      <%end%>
      <%= link_to user.fname, user_path(user), style: 'color:#4682B4' %>
    </li>
    <br>
  <% end %>
</ul>


