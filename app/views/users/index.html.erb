
<%= "Logged in as #{current_user.fname}"%><br><br>
<h1>All Users</h1>

<ul>
  <% @users.each do |user| %>
    <li>
      <%= link_to user.fname, user_path(user) %>
    </li>
  <% end %>
</ul>

<% if user_signed_in? %>
  <%= link_to "Sign out", destroy_user_session_path, method: :delete %>
<% else %>
  <%= link_to "Sign iin", new_user_session_path %>
<% end %>
